class Book{
    String title,author;
    int ISBN;
    Book(String title,String author,int ISBN){
        this.title=title;
        this.author=author;
        this.ISBN=ISBN;
    }
}
class a_exercise{
    public static void main(String args[]){
        //provide a list of the models
        Book sample[];
        sample =new Book[10];
        sample[0]=new Book("Crime and punishment", "Fyodor Dostoevsky", 0);
        sample[1]=new Book("Notes from the underground","Fyodor Dostoevsky", 1);
        sample[2]=new Book("I dream of dog's", "Samantha Mackulin", 3);
        sample[3]=new Book("The brothers karamazov", "Fyodor Dostoevsky", 4);
        sample[4]=new Book("Desiring God", "John Piper", 4);
        sample[5]=new Book("When i don't desire God", "John Piper", 5);
        sample[6]=new Book("Spectacular Sins", "John Piper ", 6);
        sample[7]=new Book("The Idiot","Fyodor Dostoevsky",7);
        sample[8]=new Book("Toast in the machine", "Samantha Mackulin", 8);
        System.out.println("Hello World!");
    }
}